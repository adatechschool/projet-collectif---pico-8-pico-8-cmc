pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
function _init()
--sprites
--cat
	c={
		score=0,
		sp=2,
		x=64,
		y=64,
		--speed=1
	}
--dog
	d={
		sp=1,
		x=rnd(120),
		y=rnd(120),
		--speed=1
	}
--fish and fishes
	fishes ={}
	for i=1,1 do
		local f={
			sp=4,
			x=flr(rnd(16))*8,
			y=flr(rnd(16))*8,	
		}
		add(fishes,f)	
	end
end
--update
function _update()
	if game_over!=true then
		if (btnp(➡️)) c.x+=4
		if (btnp(⬅️)) c.x-=4
		if (btnp(⬇️)) c.y+=4
		if (btnp(⬆️)) c.y-=4	
	end
--game limits on screen
	c.x=mid(0,120,c.x)
	c.y=mid(0,120,c.y)
--collision
	for f in all(fishes) do
		if f.x==c.x and f.y==c.y then
			del(fishes,f)
			sfx(2)
			c.score+=1
		end
	end
--print score and time
	if c.score==5 and finish_time==nil then
		finish_time=time()
		game_over=true
	end
end
--draw
function _draw()
	cls()
	map(0,0,0,0,16,16)
	spr(c.sp,c.x,c.y)
	spr(d.sp,d.x,d.y)
	for f in all(fishes) do
		spr(f.sp,f.x,f.y)
	end
--score
	print('score : '..c.score,1,1,7)
	print('time : '..flr(time()),40,1,7)
--finished !
	if game_over==true then
		print('you win !',48,48,8)
		print('it took you'..finish_time..'seconds.')	
	end
end

