-- functions sprites

function create_cat()
 for i=1,1 do
		local c={
			sp=2,
			xt=8,
			yt=3,
			flip_x=true	
		}
		add(cats,c)	
	end
end

function create_dog()
	for i=1,1 do
		local d={
			sp=1,
			xt=flr(rnd(16)),
			yt=flr(rnd(16)),	
		}
			repeat 
	 		d.xt=flr(rnd(16))
				d.yt=flr(rnd(16))
			until (check_flag(0,
																					d.xt,
																					d.yt)==false) 		
		add(dogs,d)	
	end
end

function create_fish()
	for i=1,1 do
		local f={
			sp=4,
			xt=0,
			yt=0,	
		}
		repeat 
	 	f.xt=flr(rnd(16))
			f.yt=flr(rnd(16))
		until (check_flag(0,
																				f.xt,
																				f.yt)==false) 		
		add(fishies,f)	
	end
end
					

function draw_sprites()
	for c in all(cats) do
		spr(c.sp,c.xt*8,c.yt*8,1,1,c.flip_x) 
	end
	for f in all(fishies) do
		spr(f.sp,f.xt*8,f.yt*8)
	end
	for d in all(dogs) do
		spr(d.sp,d.xt*8,d.yt*8)
	end
end








